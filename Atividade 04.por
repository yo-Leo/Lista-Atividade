programa {
  funcao inicio() {
    inteiro num1, num2

    escreva ("Digite o primeiro número: ")
    leia (num1)
    escreva ("\n")

    escreva ("Digite o segundo número: ")
    leia (num2)
    escreva ("\n")

    se (num1 > num2) {
    escreva (num1, " é o maior")
    }
    
    senao {
      escreva (num2, " é o maior")
    } 

    
  }
}
